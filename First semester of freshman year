#include <stdio.h>
#include <stdlib.h>
#define MAXNUM 100
//输入数组元素
void inputData(double*, int);
//对数组进行排序（升序或降序）
void sortData(double*, int, int);
//显示排序结果
void outputData(double*, int);
//swap函数，用于元素交换
void swap(double*,double*);

void inputData(double* data, int N)
{
    int i;
    for (i = 0; i < N; i++)
    {
        scanf_s("%lf", data++);
    }
}

void sortData(double* data, int N, int mode)
{
    int i, j;
    if (mode == 0)
    {
        for (i = 0; i < N; i++, data+i)
        {
            for (j = 0; j < N - 1; j++, data + j)
            {
                swap(data + j, data + j + 1);
            }
        }
    }
    else
    {
        for (i = N - 1; i >= 1; i--, data + i)
        {
            for (j = N - 1; j >= 1; j--, data + j)
            {
                swap(data + j, data + j - 1);
            }
        }

    }
}

void outputData(double* data, int N)
{
    for (N; N > 0; N--)
    {
        printf("%.2lf ", *data++);
    }
}

void swap(double* data1, double* data2)
{
    double temp;
    if (*data1 <= *data2)
    {
        data1 = data1;
        data2 = data2;
    }
    else if (*data1 > *data2)
    {
        temp = *data1;
        *data1 = *data2;
        *data2 = temp;
    }
}

int main()
{
    //mode:排序方式，为0表示升序，非0表示降序
    int N, mode;
    int k;
    double data[MAXNUM];
    //输入
    scanf_s("%d%d", &N, &mode);
    inputData(data, N);
    //排序
    sortData(data, N, mode);
    //显示排序结果
    outputData(data, N);
    return 0;
}
